      <fieldset style="display: inline">
        <legend>Pressure</legend>
        <div>QFE: <span id="qfe"></span> hPa measured at <span id="qfe_h"></span> m</div>
        <div>QNH: <span id="qnh"></span> hPa (ISA height <span id="isa_h"></span> m)</div>
      </fieldset>
      <fieldset style="display: inline">
        <legend>Temperatures</legend>
        <div>Wind Temperature: <span id="wind_temperature"></span> °C</div>
        <div>Temperature: <span id="temperature"></span> °C</div>
        <div>Rel. Humidity <span id="humidity"></span>%</div>
        <div>Dew Point <span id="dewpoint"></span> °C</div>
      </fieldset>
    </div>


